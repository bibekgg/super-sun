{{!-- services --}}
<div class="section-block-grey">
	<div class="container">
		<div class="center-holder section-heading">
			<h2>Our Services</h2>
			<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor<br>incididunt ut labore et
				dolore magna aliqua.
		</div>
		<div class="row">
			{{# each data.services}}
			<div class="col-xs-12 col-md-4 col-sm-4">
				<div class="service-block">
					{{#if image.url}}
					<img alt="service-image" src="{{{cloudinaryUrl image  width=342 height=200}}}">
					{{else}}
					<img alt="blog-img" src="https://via.placeholder.com/342x200?text={{name}}">
					{{/if}}
					<div class="clearfix inner-padd">
						<h4>{{name}}</h4>
						<p>{{description}}</p>
					</div>
				</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>
