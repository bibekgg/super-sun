{{!-- testimonials --}}
<div class=section-block-parallax style="background-image:url(images/parallax-4.jpg)">
	<div class=container>
		<div class="center-holder section-heading white-color">
			<h2>What our Clients Say</h2>
		</div>
		<div class="owl-carousel owl-theme" id=testmonials>
			{{# each data.testimonials}}
			<div class="clearfix testmonial-block">
				<div class=testmonial-img>
					{{#if image.url}}
					<img alt="User Image"
						src="{{{cloudinaryUrl image width=150 height=150 crop="thumb" gravity="face"}}}">
					{{else}}
					<img alt="blog-img" src="https://via.placeholder.com/150?text={{name}}">
					{{/if}}
				</div>
				<div class=testmonial-text>
					<div class=ml-15><span>{{company}}</span> <strong>{{name}}</strong><span>{{date postedDate}}</span>
						{{{message}}}
					</div>
				</div>
			</div>
			{{/each}}
		</div>
	</div>
</div>
{{!-- partners --}}
<div class="partner-section">
	<div class="container">
		<div class="center-holder section-heading">
			<h2>Our Clients</h2>
		</div>
		<div class="owl-carousel owl-theme partners" id="our-partners">
			{{# each data.clients}}
			<div class=item> <img alt=partner-image src="{{{cloudinaryUrl image}}}"> </div>
			{{/each}}
		</div>
	</div>
</div>